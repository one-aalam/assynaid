{
  "name": "assynaid",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && node scripts/build.js && node scripts/verify-build.js",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "package": "VERSION=$npm_package_version node scripts/package.js",
    "package:release": "npm run lint && npm run check && npm run package",
    "clean": "rimraf dist node_modules releases",
    "start": "npm run dev",
    "verify-build": "node scripts/verify-build.js",
    "prepare:release": "node scripts/prepare-release.js"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@tsconfig/svelte": "^5.0.2",
    "@types/chrome": "^0.0.254",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "archiver": "^6.0.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "prettier-plugin-svelte": "^3.1.2",
    "rimraf": "^5.0.5",
    "svelte": "^4.2.9",
    "svelte-check": "^3.6.2",
    "svelte-preprocess": "^5.1.3",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  },
  "dependencies": {
    "bits-ui": "^0.17.0",
    "clsx": "^2.1.0",
    "lucide-svelte": "^0.307.0",
    "mode-watcher": "^0.1.2",
    "svelte-sonner": "^0.3.10",
    "tailwind-merge": "^2.2.0",
    "tailwind-variants": "^0.1.20"
  }
}
